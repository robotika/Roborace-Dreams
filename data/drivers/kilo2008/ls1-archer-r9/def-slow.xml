<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Archer R9" type="template">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="6" max="10" val="10"/>
	</section>

	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="22" val="20"/>
	</section>

	<section name="Gearbox">
		<section name="gears">
		<section name="r">
			<attnum name="ratio" min="-3" max="0" val="-3.0"/>
		</section>

		<section name="1">
			<attnum name="ratio" min="0" max="9" val="3.0"/>
		</section>

		<section name="2">
			<attnum name="ratio" min="0" max="5" val="2.50"/>
		</section>

		<section name="3">
			<attnum name="ratio" min="0" max="5" val="2.10"/>
		</section>

		<section name="4">
			<attnum name="ratio" min="0" max="5" val="1.70"/>
		</section>

		<section name="5">
			<attnum name="ratio" min="0" max="5" val="1.40"/>
		</section>

		<section name="6">
			<attnum name="ratio" min="0" max="5" val="1.20"/>
		</section>
	</section>
	</section>

	<section name="Rear Differential">
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="1" max="10" val="3.5"/>
		<attnum name="max slip bias" min="0.0" max="1.0" val="0.3"/>
	</section>

	<section name="Steer">
		<attnum name="steer lock" unit="deg" min="1" max="25" val="25"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.9" val="0.6"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="45000"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.5"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3.0"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.5"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3.0"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="75"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0.0"/>
		<attnum name="camber" min="-4" max="0" unit="deg" val="-2.0"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="140" val="75"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0.0"/>
		<attnum name="camber" min="-4" max="0" unit="deg" val="-2.0"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="30"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="210"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="16"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="32"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="5"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="10"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="210"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="16"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="32"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="5"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="10"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" min="90" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="12"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="24"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="8"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" min="90" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.075"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="12"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="24"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="8"/>
	</section>

</params>

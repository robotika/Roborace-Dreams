<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="mpa11-murasama" type="template" mode="mw">
	<section name="Front Wing">	 
		<attnum name="angle" unit="deg" min="0" max="29" val="8"/>	 
	</section>	 
		 
	<section name="Rear Wing">	 
	    	<attnum name="angle" unit="deg" min="0" max="29" val="5"/>	 
	</section>	 

	<section name="private">
		<attnum name="base speed" min="0.0" max="50.0" val="28.00"/>
		<attnum name="base brake" min="0.0" max="50.0" val="30.00"/>
		<attnum name="add avoid speed" min="-20" max="20" val="3.0"/>
		<attnum name="avoid offset" min="0.0" max="2.0" val="1.0"/>
    	<attnum name="int margin" val="1.72"/>
    	<attnum name="ext margin" val="1.3"/>
    	<attnum name="offtrack allowed" val="0"/>
	</section>

</params>















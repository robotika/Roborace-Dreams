<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="sc-cavallo-360" type="template" mode="mw">
	<section name="Brake System">
    	<attnum name="front-rear brake repartition" min="0.3" max="0.65" val="0.485"/>
    	<attnum name="max pressure" unit="kPa" min="5000" max="50000" val="39000"/>
	</section>
	
  <section name="private">
  	<attnum name="fuel per meters" val="0.0005"/>
	<attnum name="kz scale" val="0.05"/>
	<attnum name="mu scale" val="0.85"/>
	<attnum name="brake scale" val="1.35"/>
	<attnum name="bump factor" val="0.1"/>
	<attnum name="bump factor left" val="0.234"/>
	<attnum name="bump factor right" val="0.3"/>
	<attnum name="factor 0" val="1.015"/>
	<attnum name="steer k acc" val="3"/>
	<attnum name="steer k dec" val="0"/>
	<attnum name="spd ctrl normal" val="6"/>
	<attnum name="spd ctrl traffic" val="4"/>
	<attnum name="spd ctrl extra" val="6"/>
	<attnum name="steer ctrl" val="3"/>
  </section>

</params>















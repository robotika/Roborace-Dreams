<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="sc-spirit-300" type="template" mode="mw">
	<section name="Features">
		<attstr name="enable abs" in="yes,no" val="yes"/>
		<attstr name="enable tcl" in="yes,no" val="yes"/>
		<attstr name="enable esp" in="yes,no" val="yes"/>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.5" max="0.65" val="0.55"/>
		<attnum name="max pressure" unit="kPa" min="1000" max="20000" val="17000"/>
	</section>
	
	<section name="private">
		<attnum name="shift" val="0.99"/>
		<attnum name="kz scale" val="0.55"/>
		<attnum name="bump factor" val="0.1"/>
		<attnum name="mu scale" val="1.05"/>
		<attnum name="factor 0" val="1.001"/>
		<attnum name="steer k acc" val="3"/>
		<attnum name="steer k dec" val="0"/>
		<attnum name="spd ctrl normal" val="3"/>
		<attnum name="spd ctrl traffic" val="4"/>
		<attnum name="spd ctrl extra" val="6"/>
		<attnum name="steer ctrl" val="0"/>
		<attnum name="full fuel" val= "0"/>
	</section>

</params>















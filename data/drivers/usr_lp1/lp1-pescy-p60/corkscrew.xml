<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ls1-taipan-ltsr" type="template" mode="mw">

	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="7" max="17" val="17.0"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="4" max="30" val="23.0"/>
	</section>

	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2.0"/>
			</section>

			<section name="1">
				<attnum name="ratio" min="0" max="5" val="2.66"/>
			</section>

			<section name="2">
				<attnum name="ratio" min="0" max="5" val="1.78"/>
			</section>

			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.348"/>
			</section>

			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.091"/>
			</section>

			<section name="5">
				<attnum name="ratio" min="0" max="5" val="0.919"/>
			</section>

			<section name="6">
				<attnum name="ratio" min="0" max="5" val="0.832"/>
			</section>
		</section>
	</section>

	<section name="Rear Differential">
		<attnum name="ratio" min="1.0" max="10.0" val="4.6"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.57"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="30000"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="13"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="10"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="75"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="0"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.15"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="13"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="26"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="9"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.15"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="13"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="26"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="9"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.15"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="11"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="22"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="7"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" min="100" max="410" unit="kN/m" val="130"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.15"/>
		<attnum name="bellcrank" val="1.0"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="kN/m/s" min="0" max="70" val="11"/>
		<attnum name="slow rebound" unit="kN/m/s" min="0" max="70" val="22"/>
		<attnum name="fast bump" unit="kN/m/s" min="0" max="70" val="4"/>
		<attnum name="fast rebound" unit="kN/m/s" min="0" max="70" val="7"/>
		<attnum name="bump limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
		<attnum name="rebound limit velocity" unit="m/s" min="0.1" max="0.3" val="0.2"/>
	</section>

	<section name="private">
		<attnum name="base speed factor" val="1.11"/>
		<attnum name="base brake factor" val="1.56"/>
	
		<attnum name="0 bgn" val="320"/>
		<attnum name="0 end" val="570"/>
		<attnum name="0 base speed" val="21.00"/>
		<attnum name="0 brake mod" val="1.00"/>
	
		<attnum name="1 bgn" val="570"/>
		<attnum name="1 end" val="750"/>
		<attnum name="1 int margin" val="1.20"/>
		<attnum name="1 brake mod" val="1.00"/>
		<attnum name="1 base speed" val="22.00"/>
		<attnum name="1 base brake factor" val="1.75"/>
		
		<attnum name="2 bgn" val="1270"/>
		<attnum name="2 end" val="1330"/>
		<attnum name="2 int margin" val="1.40"/>
		<attnum name="2 base speed factor" val="1.12"/>
		
		<attnum name="3 bgn" val="1370"/>
		<attnum name="3 end" val="1610"/>
		<attnum name="3 base speed" val="15.00"/>
	</section>
</params>

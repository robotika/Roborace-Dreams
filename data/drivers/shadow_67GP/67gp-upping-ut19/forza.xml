<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="67gp-upping-ut19" type="template" mode="mw">
	<section name="Features">
		<attstr name="tire temperature and degradation" in="yes" val="yes"/>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.75" val="0.535"/>
		<attnum name="max pressure" unit="kPa" min="8000" max="18000" val="18000"/>
	</section>
	
	<section name="private">
		<attnum name="gear up rpm" val="8300"/>
		<attnum name="kz scale" val="0.55"/>
		<attnum name="mu scale" val="1.00"/>
		<attnum name="factor 0" val="1.005"/>
		<attnum name="steer k acc" val="3"/>
		<attnum name="steer k dec" val="0"/>
		<attnum name="spd ctrl normal" val="6"/>
		<attnum name="spd ctrl traffic" val="4"/>
		<attnum name="spd ctrl extra" val="6"/>
		<attnum name="steer ctrl" val="0"/>
		<attnum name="full fuel" val= "90"/>
	</section>

</params>















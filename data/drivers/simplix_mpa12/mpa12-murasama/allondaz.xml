	<attnum name="factor 0" val="1.055"/>
	<attnum name="steer k acc" val="3"/>
	<attnum name="steer k dec" val="0"/>
	<attnum name="spd ctrl normal" val="3"/>
	<attnum name="spd ctrl traffic" val="3"/>
  </section>

</params>















		<attnum name="front-rear brake repartition" min="0.4" max="0.7" val="0.490"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="50000" val="30500"/>
	</section>
	
  <section name="private">
	<attnum name="kz scale" val="0.55"/>

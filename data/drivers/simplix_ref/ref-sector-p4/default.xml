<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ref-sector-p4" type="template" mode="mw">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="0" val="0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="22" val="6"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-3"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="4.04"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.65"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.93"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.5"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.18"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.92"/>
      </section>

      <section name="7">
        <attnum name="ratio" min="0" max="5" val="0.7"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="FREE,LIMITED SLIP" val="FREE"/>
    <attnum name="ratio" min="3.308" max="3.308" val="3.308"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.45" max="0.65" val="0.65"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="11000" val="10800"/>
    <attnum name="brake correction" min="0.03" max="0.3" val="0.12"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="14.08"/>
    <attnum name="suspension course" unit="m" min="0.2" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="25.94"/>
    <attnum name="suspension course" unit="m" min="0.2" max="0.2" val="0.2"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="120" max="140" val="120"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="120" max="140" val="120"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="140" max="160" val="160"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-2.5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="140" max="160" val="160"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="50"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.28"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="0" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0.1" max="100" val="4.97"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0.1" max="100" val="2.81"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="100" val="0.88"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="100" val="0.37"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="50"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.28"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="0" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0.1" max="100" val="4.97"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0.1" max="100" val="2.81"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="100" val="0.88"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="100" val="0.37"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="74.07"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.28"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="0" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0.1" max="100" val="4.03"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0.1" max="100" val="5.05"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="100" val="0.69"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="100" val="0.94"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="74.07"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.28"/>
    <attnum name="bellcrank" min="1" max="1" val="1"/>
    <attnum name="packers" unit="mm" min="0" max="0" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="0.1" max="100" val="4.03"/>
    <attnum name="slow rebound" unit="kN/m/s" min="0.1" max="100" val="5.05"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="100" val="0.69"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="100" val="0.94"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="72" val="-1"/>
    <attnum name="scale brake" min="0.5" max="1" val="0.70"/>
    <attnum name="scale mu" min="0.5" max="1" val="0.88"/>
    <attnum name="scale bump" min="0.001" max="0.6" val="0.3"/>
    <attnum name="base mode" min="0.97" max="1.03" val="0.99224"/>
    <attnum name="base scale" min="1" max="1.05" val="1.00899"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
    <attnum name="lookaheadfactor" min="0.01" max="0.2" val="0.06"/>
  </section>
</params>

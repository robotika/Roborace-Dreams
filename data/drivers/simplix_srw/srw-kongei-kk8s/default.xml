<!DOCTYPE params SYSTEM "params.dtd">


<params name="srw-kongei-kk8s">
  <section name="Features">
    <attstr name="shifting aero coordinate" in="yes" val="yes"/>
    <attstr name="fixed low speed grip" in="yes" val="yes"/>
    <attstr name="realistic gear change" in="yes,no" val="yes"/>
    <attstr name="realistic rev limiter" in="yes" val="yes"/>
    <attstr name="tire temperature and degradation" in="yes" val="yes"/>
    <attstr name="fixed wheel force" in="yes" val="yes"/>
    <attstr name="enable cliftlimit" in="yes" val="yes"/>
    <attstr name="enable abs" in="yes,no" val="yes"/>
    <attstr name="enable tcl" in="yes,no" val="yes"/>
    <attstr name="enable esp" in="yes,no" val="yes"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="25" val="5.15"/>
  </section>

  <section name="Rear Differential">
    <attstr name="type" in="FREE,LIMITED SLIP" val="FREE"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.3557"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="24000" val="22000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="0"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="46.55"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="80"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0.15"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="80"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0.15"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="160" val="80"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0.12"/>
    <attnum name="camber" unit="deg" min="-2.5" max="0" val="-2.5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="160" val="80"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0.12"/>
    <attnum name="camber" unit="deg" min="-2.5" max="0" val="-2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="74.5"/>
    <attnum name="suspension course" unit="m" min="0.12" max="0.56" val="0.16"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="4.88"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="4.05"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="10" val="1.92"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="10" val="0.86"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="74.5"/>
    <attnum name="suspension course" unit="m" min="0.12" max="0.56" val="0.16"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="4.88"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="4.05"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="10" val="1.92"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="10" val="0.86"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="147.8"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.56" val="0.2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="4"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="5.04"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="10" val="1.14"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="10" val="0.95"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="147.8"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.56" val="0.2"/>
    <attnum name="packers" unit="mm" min="0" max="10" val="0"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="4"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="5.04"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="10" val="1.14"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="10" val="0.95"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="80" val="-1"/>
    <attnum name="scale brake" min="0.8" max="1.2" val="0.93"/>
    <attnum name="scale mu" min="0.8" max="1.2" val="0.86"/>
    <attnum name="scale bump" min="0.001" max="2" val="0.224"/>
    <attnum name="base mode" min="0.95" max="1.02" val="0.98191"/>
    <attnum name="base scale" min="1" max="1.05" val="1.00327"/>
    <attnum name="lookahead" min="2.0" max="4.0" val="2.5"/>
    <attnum name="lookaheadfactor" min="0.10" max="0.25" val="0.143"/>
    <attnum name="shift up" min="0.8" max="0.97" val="0.89"/>
    <attnum name="shift margin" min="0.8" max="0.95" val="0.9"/>
    <attnum name="clutch delta" min="0.001" max="1" val="0.008"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
    <attnum name="sin long" min="0" max="1" val="1"/>
  </section>

</params>

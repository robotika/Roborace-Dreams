<!DOCTYPE params SYSTEM "params.dtd">


<params name="srw-boxer-ts">
  <section name="Features">
    <attstr name="shifting aero coordinate" in="yes" val="yes"/>
    <attstr name="fixed low speed grip" in="yes" val="yes"/>
    <attstr name="realistic gear change" in="yes,no" val="yes"/>
    <attstr name="realistic rev limiter" in="yes" val="yes"/>
    <attstr name="tire temperature and degradation" in="yes" val="yes"/>
    <attstr name="fixed wheel force" in="yes" val="yes"/>
    <attstr name="enable cliftlimit" in="yes" val="yes"/>
    <attstr name="enable abs" in="yes,no" val="yes"/>
    <attstr name="enable tcl" in="yes,no" val="yes"/>
    <attstr name="enable esp" in="yes,no" val="yes"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.45" max="0.65" val="0.5061"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="40000" val="22000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="0.43"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="49.3"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="70"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0.04"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="140" val="70"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0.04"/>
    <attnum name="camber" unit="deg" min="-5" max="0" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" unit="mm" min="60" max="160" val="70"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="-0"/>
    <attnum name="camber" unit="deg" min="-2.5" max="0" val="-2.5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" unit="mm" min="60" max="160" val="70"/>
    <attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
    <attnum name="camber" unit="deg" min="-2.5" max="0" val="-2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="109.1"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.155"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="5.51"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="2.66"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="2" val="0.5"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="2" val="0.31"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="109.1"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.155"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="5.51"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="2.66"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="2" val="0.5"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="2" val="0.31"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="129.7"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.195"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="4.51"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="5.01"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="2" val="1.11"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="2" val="1.02"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="50" max="150" val="129.7"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.28" val="0.195"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="10" val="4.51"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="10" val="5.01"/>
    <attnum name="fast bump" unit="kN/m/s" min="0.1" max="2" val="1.11"/>
    <attnum name="fast rebound" unit="kN/m/s" min="0.1" max="2" val="1.02"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="68" val="-1"/>
    <attnum name="scale brake" min="0.8" max="1.2" val="0.92"/>
    <attnum name="scale mu" min="0.8" max="1.2" val="0.85"/>
    <attnum name="scale bump" min="0.001" max="1" val="0.161"/>
    <attnum name="base mode" min="0.96" max="1.04" val="0.98983"/>
    <attnum name="base scale" min="1" max="1.03" val="1.00358"/>
    <attnum name="lookahead" min="1" max="10" val="3"/>
    <attnum name="lookaheadfactor" min="0.01" max="0.5" val="0.15"/>
    <attnum name="shift up" min="0.8" max="0.99" val="0.915"/>
    <attnum name="shift margin" min="0.7" max="1" val="0.9"/>
    <attnum name="clutch delta" min="0.001" max="1" val="0.043"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
  </section>

</params>

<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="mpa12-murasama" type="template" mode="mw">
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.4" max="0.7" val="0.535"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="50000" val="40000"/>
	</section>
	
	<section name="private">
    <attnum name="fuelperlap" val="2.25"/>
    <attnum name="avoid speed factor" val="0.88"/>
    <attnum name="avoid brake factor" val="0.80"/>
    <attnum name="exit boost factor" val="1.161"/>
    <attnum name="base speed" val="14.00"/>
    <attnum name="base brake" val="24.06"/>
	<attnum name="brake margin" val="1.5"/>
    <attnum name="slope factor" val="0.54"/>
    <attnum name="accel exit" val="0.4"/>
    <attnum name="turn decel" val="0.4"/>
    <attnum name="revs change up" val="0.90"/>
    <attnum name="revs change down" val="0.67"/>
    <attnum name="revs change down max" val="0.74"/>
    <attnum name="int margin" val="1.7"/>
    <attnum name="ext margin" val="1.3"/>
    <attnum name="abs slip" val="2"/>
    <attnum name="tcl slip" val="4"/>
    <attnum name="bump caution" val="0.4"/>
    <attnum name="raceline debug" val="1"/>
	</section>

</params>















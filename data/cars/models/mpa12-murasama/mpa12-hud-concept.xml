			<attstr name="color red" val="0.051"/>
			<attstr name="color green" val="0.525"/>
			<attstr name="color blue" val="0.396"/>
		</section>
		
		<!-- background color when car lights are on (for GT cars and night racing)
		
		<section name="color night">
			<attstr name="color red" val="0.051"/>
			<attstr name="color green" val="0.525"/>
			<attstr name="color blue" val="0.396"/>
		</section>
		
		-->
		
